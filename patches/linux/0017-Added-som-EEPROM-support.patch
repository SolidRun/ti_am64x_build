From a5ab947077a04ca37ed71696de507881653ca473 Mon Sep 17 00:00:00 2001
From: Kareiman Fadly <kareiman.fadly@solid-run.com>
Date: Fri, 2 Dec 2022 14:33:21 +0200
Subject: [PATCH 17/17] Added som EEPROM support

---
 arch/arm64/boot/dts/ti/am642-solidrun.dts | 9 ++++++++-
 1 file changed, 8 insertions(+), 1 deletion(-)

diff --git a/arch/arm64/boot/dts/ti/am642-solidrun.dts b/arch/arm64/boot/dts/ti/am642-solidrun.dts
index 46b1c15db..05afd75c4 100644
--- a/arch/arm64/boot/dts/ti/am642-solidrun.dts
+++ b/arch/arm64/boot/dts/ti/am642-solidrun.dts
@@ -162,7 +162,7 @@ led-0 {
 			linux,default-trigger = "heartbeat";
 			function = LED_FUNCTION_HEARTBEAT;
 			default-state = "off";
-		};
+    };
 	};
 
 	icssg1_eth: icssg1-eth {
@@ -498,6 +498,13 @@ &main_i2c0 {
 	pinctrl-names = "default";
 	pinctrl-0 = <&i2c0_pins>;
 	clock-frequency = <400000>;
+
+
+  som_eeprom: eeprom@50 {
+		compatible = "atmel,24c02";
+		reg = <0x50>;
+    pagesize = <16>;
+  };
 };
 
 
