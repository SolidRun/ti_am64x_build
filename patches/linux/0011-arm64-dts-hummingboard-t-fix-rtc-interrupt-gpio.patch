From 203aec190f84295b42c3576b1df3dcbcd7837f7a Mon Sep 17 00:00:00 2001
From: Josua Mayer <josua@solid-run.com>
Date: Sun, 21 May 2023 16:31:02 +0300
Subject: [PATCH] arm64: dts: hummingboard-t: fix rtc interrupt gpio

Signed-off-by: Josua Mayer <josua@solid-run.com>
---
 arch/arm64/boot/dts/ti/k3-am642-hummingboard-t.dts | 8 ++++----
 1 file changed, 4 insertions(+), 4 deletions(-)

diff --git a/arch/arm64/boot/dts/ti/k3-am642-hummingboard-t.dts b/arch/arm64/boot/dts/ti/k3-am642-hummingboard-t.dts
index 76d9971760c7..29478ffc8701 100644
--- a/arch/arm64/boot/dts/ti/k3-am642-hummingboard-t.dts
+++ b/arch/arm64/boot/dts/ti/k3-am642-hummingboard-t.dts
@@ -445,9 +445,9 @@ light_sensor: opt3002@44 {
 	/* SoM */
 	som_eeprom: eeprom@50 {
 		status = "okay";
-		compatible = "atmel,24c02";
+		compatible = "atmel,24c01";
 		reg = <0x50>;
-		pagesize = <16>;
+		pagesize = <8>;
 	};
 
 	/* Carrier */
@@ -485,7 +485,7 @@ rtc: am1805aq@69 {
 		pinctrl-0 = <&main_i2c1_rtc_pins_default>;
 		pinctrl-names = "default";
 		interrupt-parent = <&main_gpio0>;
-		interrupts = <36 IRQ_TYPE_EDGE_FALLING>;
+		interrupts = <44 IRQ_TYPE_EDGE_FALLING>;
 	};
 };
 
@@ -596,7 +596,7 @@ AM64X_IOPAD(0x026c, PIN_INPUT, 0)	/* Pad I2C1_SDA Mux I2C1_SDA */
 	main_i2c1_rtc_pins_default: main-i2c1-rtc-pins-default {
 		pinctrl-single,pins = <
 			/* external pull-up on Carrier */
-			AM64X_IOPAD(0x0094, PIN_INPUT, 7)	/* Pad GPMC0_BE1n Mux GPIO0_36 */
+			AM64X_IOPAD(0x00b4, PIN_INPUT, 7)	/* Pad GPMC0_CSn3 Mux GPIO0_44 */
 		>;
 	};
 
-- 
2.35.3

